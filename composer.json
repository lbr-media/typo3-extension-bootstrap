{
    "name": "lbr-media/typo3-extension-bootstrap",
    "type": "typo3-cms-extension",
    "description": "Typo3 template extension with Twitter Bootstrap 5 package.",
    "homepage": "https://github.com/lbr-media/typo3-extension-bootstrap",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Marcel",
            "email": "mb@lbrmedia.de",
            "role": "Developer"
        }
    ],
    "require": {
        "erusev/parsedown": "^1.7",
        "php": ">=8.1.0",
        "twbs/bootstrap": "^5.1",
        "twbs/bootstrap-icons": "^1.8",
        "typo3/cms-backend": "^12.0",
        "typo3/cms-belog": "^12.0",
        "typo3/cms-core": "^12.0",
        "typo3/cms-extbase": "^12.0",
        "typo3/cms-filemetadata": "^12.0",
        "typo3/cms-frontend": "^12.0",
        "typo3/cms-recordlist": "^12.0",
        "typo3/cms-rte-ckeditor": "^12.0",
        "typo3/cms-seo": "^12.0",
        "typo3/cms-setup": "^12.0",
        "typo3/cms-tstemplate": "^12.0",
        "typo3/cms-viewpage": "^12.0"
    },
    "require-dev": {
        "helhum/typo3-console": "^7.0.2",
        "typo3/cms-lowlevel": "^12.0"
    },
    "suggest": {
        "lbr-media/typo3-bootstrap-base": "^1.0",
        "lbr-media/typo3-extension-bootstrap-example": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        },
        "typo3/cms": {
            "extension-key": "bootstrap"
        }
    },
    "autoload": {
        "psr-4": {
            "LBRmedia\\Bootstrap\\": "Classes"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    }
}
