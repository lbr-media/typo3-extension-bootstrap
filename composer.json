{
    "name": "lbr-media/typo3-extension-bootstrap",
    "type": "typo3-cms-extension",
    "description": "Twitter Bootstrap package",
    "homepage": "https://github.com/lbr-media/typo3-extension-bootstrap",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Marcel",
            "email": "mb@lbrmedia.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.1",
        "twbs/bootstrap": "^5.1",
        "typo3/cms-backend": "^11.5",
        "typo3/cms-belog": "^11.5",
        "typo3/cms-core": "^11.5",
        "typo3/cms-extbase": "^11.5",
        "typo3/cms-filemetadata": "^11.5",
        "typo3/cms-frontend": "^11.5",
        "typo3/cms-recordlist": "^11.5",
        "typo3/cms-rte-ckeditor": "^11.5",
        "typo3/cms-seo": "^11.5",
        "typo3/cms-setup": "^11.5",
        "typo3/cms-tstemplate": "^11.5",
        "typo3/cms-viewpage": "^11.5"
    },
    "require-dev": {
        "helhum/typo3-console": "^7.0.2",
        "typo3/cms-lowlevel": "^11.5"
    },
    "suggest": {
        "lbr-media/typo3-bootstrap-base": "^1.0",
        "lbr-media/typo3-extension-bootstrap-example": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "typo3/cms": {
            "extension-key": "bootstrap"
        }
    },
    "autoload": {
        "psr-4": {
            "LBRmedia\\Bootstrap\\": "Classes"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    }
}
