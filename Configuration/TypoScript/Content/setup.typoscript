
# Content
lib.contentElement = FLUIDTEMPLATE
lib.contentElement {
    templateName = Default

    layoutRootPaths {
        100 = {$plugin.tx_bootstrap.view.layoutRootPath}Content/
    }
    partialRootPaths {
        100 = {$plugin.tx_bootstrap.view.partialRootPath}Content/
    }
    templateRootPaths {
        100 = {$plugin.tx_bootstrap.view.templateRootPath}Content/
    }

    settings {
        defaultHeaderType = 2
    }
}

