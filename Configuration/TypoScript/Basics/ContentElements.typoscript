                references.fieldName = assets
            }

            20 = LBRmedia\Bootstrap\DataProcessing\FlexFormProcessor
            20 {
                type = TYPE_TT_CONTENT_BOOTSTRAP_TEXT_MEDIA_GRID
            }
        }
    }
}