    }
    bootstrap_textmediagrid =< lib.contentElement
    bootstrap_textmediagrid {
        templateName = BootstrapTextMediaGrid
        dataProcessing {
            10 = TYPO3\CMS\Frontend\DataProcessing\FilesProcessor
