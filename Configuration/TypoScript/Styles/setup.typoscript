plugin.tx_bootstrap._CSS_DEFAULT_STYLE (
    /*
    copyright not in images
    */
    
    .figure-copyright {
        position: absolute;
        bottom: 0;
        right: 0;
        padding: 0.35em 0.5em;
        color: rgba(255, 255, 255, 0.8) !important;
        background-color: rgba(55, 55, 55, 0.7) !important;
        font-size: 10px;
        line-height: 12px;
        max-width: 100%;
        max-height: 100%;
        overflow: hidden;
        white-space: nowrap;
    }

    .figure-copyright--bottom-left {
        right: auto;
        left: 0;
    }

    .figure-copyright--top-left {
        right: auto;
        left: 0;
        top: 0;
    }

    .figure-copyright--top-right {
        bottom: auto;
        top: 0;
    }

    .figure-copyright::before {
        content: "Â©";
        margin-right: 0.25em;
    }

