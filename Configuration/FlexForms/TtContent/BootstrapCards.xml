                                </range>
                                <default>100</default>
                                <slider>
                                    <step>1</step>
                                    <width>200</width>
                                </slider>
                            </config>
                        </value>
                    </field>
                    <field index="width_lg">
                        <value index="TCEforms">
                            <label>LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:sMEDIAOPTIMIZING.width_lg</label>
                            <config>
                                <type>input</type>
                                <eval>int</eval>
                                <range>
                                    <lower>1</lower>
                                    <upper>100</upper>
                                </range>
                                <default>100</default>
                                <slider>
                                    <step>1</step>
                                    <width>200</width>
                                </slider>
                            </config>
                        </value>
                    </field>
                    <field index="width_xl">
                        <value index="TCEforms">
                            <label>LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:sMEDIAOPTIMIZING.width_xl</label>
                            <config>
                                <type>input</type>
                                <eval>int</eval>
                                <range>
                                    <lower>1</lower>
                                    <upper>100</upper>
                                </range>
                                <default>100</default>
                                <slider>
                                    <step>1</step>
                                    <width>200</width>
                                </slider>
                            </config>
                        </value>
                    </field>
                    <field index="width_xxl">
                        <value index="TCEforms">
                            <label>LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:sMEDIAOPTIMIZING.width_xxl</label>
                            <config>
                                <type>input</type>
                                <eval>int</eval>
                                <range>
                                    <lower>1</lower>
                                    <upper>100</upper>
                                </range>
                                <default>100</default>
                                <slider>
                                    <step>1</step>
                                    <width>200</width>
                                </slider>
                            </config>
                        </value>
                    </field>
                </el>
            </ROOT>
        </sMEDIAOPTIMIZING>
    </sheets>
</T3FlexForms>