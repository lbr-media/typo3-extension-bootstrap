<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3FlexForms>
    <sheets>
        <sGENERAL>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Position und Abstand</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <field index="presets">
                        <value index="TCEforms">
                            <label>LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:presets</label>
                            <description>LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:presets.description</description>
                            <config>
                                <type>select</type>
                                <renderType>selectMultipleSideBySide</renderType>
                                <itemsProcFunc>LBRmedia\Bootstrap\UserFunc\Tca\TtContent->itemsProcFunc</itemsProcFunc>
                                <CType>bootstrap_textmediafloat</CType>
                            </config>
                        </value>
                    </field>
                    <field index="header_position">
                        <value index="TCEforms">
                            <label>LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:textmedia.header_position</label>
                            <config>
                                <type>radio</type>
                                <items>
                                    <numIndex index="1">
                                        <numIndex index="0">LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:textmedia.header_position.above_all</numIndex>
                                        <numIndex index="1">above_all</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:textmedia.header_position.text</numIndex>
                                        <numIndex index="1">text</numIndex>
                                    </numIndex>
                                </items>
                                <default>above_all</default>
                            </config>
                        </value>
                    </field>
                    <field index="media_position">
                        <value index="TCEforms">
                            <label>LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:textmedia.media_position</label>
                            <config>
                                <type>user</type>
                                <renderType>bootstrapDevices</renderType>
                                <default>none;end;;;;</default>
                                <elementConfiguration>TextMediaPositions</elementConfiguration>
                            </config>
                        </value>
                    </field>
                    <field index="media_size">
                        <value index="TCEforms">
                            <label>LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:textmedia.media_size</label>
                            <config>
                                <type>user</type>
                                <renderType>bootstrapDevices</renderType>
                                <default>12;4;6;;;</default>
                                <elementConfiguration>TextMediaSizes</elementConfiguration>
                            </config>
                        </value>
                    </field>
                    <field index="space_x">
                        <value index="TCEforms">
                            <label>LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:textmedia.sGENERAL.space_x</label>
                            <config>
                                <type>user</type>
                                <renderType>bootstrapDevices</renderType>
                                <default>3;;4;;;</default>
                                <elementConfiguration>BootstrapGridSpaceX</elementConfiguration>
                            </config>
                        </value>
                    </field>
                    <field index="space_y">
                        <value index="TCEforms">
                            <label>LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:textmedia.sGENERAL.space_y</label>
                            <config>
                                <type>user</type>
                                <renderType>bootstrapDevices</renderType>
                                <default>3;;;;;</default>
                                <elementConfiguration>BootstrapGridSpaceY</elementConfiguration>
                            </config>
                        </value>
                    </field>
                </el>
            </ROOT>
        </sGENERAL>
        <sMEDIA>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:sMEDIA.sheetTitle</sheetTitle>
                    <sheetDescription>LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:sMEDIA.sheetDescription</sheetDescription>
                </TCEforms>
                <type>array</type>
                <el>
                    <field index="image_zoom">
                        <value index="TCEforms">
                            <label>LLL:EXT:frontend/Resources/Private/Language/locallang_ttc.xlf:image_zoom</label>
                            <config>
                                <type>check</type>
                                <renderType>checkboxToggle</renderType>
                                <default>0</default>
                            </config>
                        </value>
                    </field>
                    <field index="masonry_enabled">
                        <value index="TCEforms">
                            <label>LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:masonry_enabled</label>
                            <config>
                                <type>check</type>
                                <renderType>checkboxToggle</renderType>
                                <default>0</default>
                            </config>
                        </value>
                    </field>
                </el>
            </ROOT>
        </sMEDIA>
        <sMEDIAITEM>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:sMEDIAITEM.sheetTitle</sheetTitle>
