                            <label>LLL:EXT:bootstrap/Resources/Private/Language/flexform.xlf:sMEDIAITEM.col</label>
                            <config>
                                <type>user</type>
                                <renderType>bootstrapDevices</renderType>
                                <default>12;;;;;</default>
